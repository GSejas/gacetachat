# GacetaChat Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-proj-your-key-here

# Twitter API Credentials (get from https://developer.twitter.com/)
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_secret_key=your-twitter-api-secret
TWITTER_CONSUMER_API_KEY=your-consumer-key
TWITTER_CONSUMER_API_secret_key=your-consumer-secret

# Backend Configuration
BACKEND_IP=localhost
BACKEND_PORT=8000
TWITTER_CALLBACK_ENDPOINT=/twitter/callback

# Application Secret
APP_SECRET_API_KEY=your-secret-key-here